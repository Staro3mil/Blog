<%- include("partials/header") -%>

<h1>Home</h1>
<p><%= homeStartingContent%></p>
<% posts.forEach(function(post){ %>
    <h1><%= post.title %></h1>
    <% if(post.body.length > 100) { %>
        <%= post.body.substring(0,100)%> ...
    <%} else { %>
        <%= post.body %>  
    <%} %>
    <a href= "/posts/<%=post.title%>">Read More</a>
<%}) %>
<%- include("partials/footer") -%>